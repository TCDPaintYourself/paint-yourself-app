{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "lib": ["dom", "esnext"],
    "baseUrl": "./",
    "paths": {
      "App": ["App.tsx"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "context/*": ["context/*"],
      "env/*": ["environments/*"],
      "hooks/*": ["hooks/*"],
      "navigation/*": ["navigation/*"],
      "providers/*": ["providers/*"],
      "screens/*": ["screens/*"],
      "types": ["types.tsx"],
      "utils/*": ["utils/*"]
    }
  }
}
